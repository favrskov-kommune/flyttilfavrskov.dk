{% set pageheader_position = {
  'center': 'col-xs-10 col-md-8 col-xs-offset-1 col-md-offset-2',
  'right': 'col-xs-10 col-md-8 col-xs-offset-2 col-md-offset-4',
  'default': 'col-xs-10 col-xs-offset-1 col-md-8'
} %}
<div{{ (not attributes ? create_attribute() : attributes).addClass(['hero', 'js-hero']).setAttribute('data-video', video_oembed) }}>
  {{ background_image }}
  <div class="hero__iframe-wrapper js-hero-iframe-wrapper"></div>
  <div class="container">
    <div class="row">
      <div class="{{ pageheader_position[position] }}">
        <div class="hero__content">
          <h1 class="hero__title">{{ title }}</h1>
          <p class="hero__description">{{ teaser }}</p>
          {% if cta.url and cta.title %}
            {% include '@dds_premium/components/molecules/m-cta.html.twig' with {
              'url': cta.url,
              'text': cta.title,
              'attributes': cta.attributes
            } %}
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
